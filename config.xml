<?xml version='1.0' encoding='utf-8'?>
<widget id="fabricio_teste_teste" version="0.0.1"  android-versionCode="26" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

  <name>Teste teste SPO</name>
  <description>
      A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author email="fabriciocarneiro.4@gmail.com" href="http://cordova.io">
      Apache Cordova Team
  </author>
  <preference name="android-minSdkVersion" value="26" />
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <allow-intent href="market:*" />
</widget>
